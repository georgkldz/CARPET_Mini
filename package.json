  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:schema": "npx typescript-json-schema ./src/stores/applicationStore.ts \"SerialisedTask\" --ignoreErrors --noExtraProps --required --out ./src/schemas/JSONSchemas/SerialisedTaskSchema.schema.json && npx json-schema-to-zod -i ./src/schemas/JSONSchemas/SerialisedTaskSchema.schema.json -o ./src/schemas/zodSchemas/SerialisedTaskSchema.ts && prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "coverage": "vitest run --coverage"
  },
